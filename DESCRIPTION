Package: spathial
Type: Package
Title: Evolutionary Analysis
Version: 0.1.2
Authors@R: c(person("Erika", "Gardini", email = "erika.gardini4@unibo.it", role = c("aut", "cre")),
  person("Federico M.", "Giorgi", email = "federico.giorgi@unibo.it", role = c("aut")),
